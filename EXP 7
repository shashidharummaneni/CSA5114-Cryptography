#include <stdio.h>
#include <string.h>

#define MAX_LEN 1000

int main() {
    char ciphertext[] =
    "53‡‡†305))6*;4826)4‡.)4‡);806*;48†8¶60))85;;]8*;:‡*8†83\n"
    "(88)5*†;46(;88*96*?;8)*‡(;485);5*†2:*‡(;4956*2(5*—4)8¶8*\n"
    ";4069285);)6†8)4‡‡;1(‡9;48081;8:8‡1;48†85;4)485†528806*81 (‡9;48;(88;4(‡?34;48)4‡;161;:188;‡?;";

    int count[256] = {0};
    int len = strlen(ciphertext);

    // Count frequencies
    for (int i = 0; i < len; i++) {
        unsigned char c = ciphertext[i];
        if (c >= 32 && c <= 126) {
            count[c]++;
        }
    }

    // Display results
    printf("Frequency of symbols:\n");
    for (int i = 32; i < 127; i++) {
        if (count[i] != 0) {
            printf("%c: %d\n", i, count[i]);
        }
    }

    return 0;
}

